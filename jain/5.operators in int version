package operators;



public class Operators 
{

	public static void main (String[] args)
	
	{
		int a=70;
	
		int b=2;
	
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
		System.out.println(Math.pow(a,b));
    System.out.println(a>b);
		System.out.println(a<b);
		System.out.println(a==b);
		System.out.println(a!=b);
		
	}

}
