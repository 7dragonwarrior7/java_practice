package operators;

public class operators_in_float_version
{
	

		public static void main (String[] args)
		
		{
			float a=70f;
		
			float b=2f;
		
			System.out.println(a*b);
			System.out.println(a/b);
			System.out.println(a+b);
			System.out.println(a-b);
			System.out.println(a*b);
			System.out.println(a/b);
			System.out.println(a%b);
			System.out.println(Math.pow(a,b));
			System.out.println(a>b);
			System.out.println(a<b);
			System.out.println(a==b);
			System.out.println(a!=b);
		}

	
}
