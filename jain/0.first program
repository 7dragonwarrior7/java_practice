package test_project;

public class TestProject {

	public static void main (String[] args)
	{
		int a=10;
		System.out.println(a);
	
	}
	
}


--------------/
10
================/
package test_project;

public class TestProject {

	public static void main (String[] args)
	{
		float a=10.48f;
		System.out.println(a);
	
	}
	
}
